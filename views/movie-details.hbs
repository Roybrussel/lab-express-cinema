<img src="{{display.image}}" alt="{{display.title}}">
<div>
    <h3>{{display.title}}</h3>
    <p>Director: {{display.director}}</p>
    <p>Description: {{display.description}}</p>
    {{#each display}}<p>{{this.stars}}</p>{{/each}}
    {{#each display}}<p>{{this.showtimes}}</p>{{/each}}
    <a href="/movies">Go Back</a>
</div>